     "text": [
      "Enter a num:121\n",
      "121 is Palindrome\n"
     ]
    }
   ],
   "source": [
    "n=int(input(\"Enter a num:\"))\n",
    "t=n\n",
    "rev=0\n",
    "while(n>0):\n",
    "    r=n%10\n",
    "    rev=rev*10+r\n",
    "    n=n//10\n",
    "if(t==rev):\n",
    "    print(t,\"is Palindrome\")\n",
    "else:\n",
    "    print(\"Not palindrome\")"
   ]
